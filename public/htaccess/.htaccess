#PHP7設定 (CakePHP3の件があるので個別で設定)
#AddHandler application/x-httpd-php71cgi .php

# .htaccessでRewriteを有効化する
RewriteEngine On
RewriteBase /public/htaccess/
RewriteCond %{REQUEST_FILE} -f
RewriteRule ^ - [QSA,L]

RewriteRule ^/?(.+)/?(.+)$ index.php?$1=$2 [QSA,L]
